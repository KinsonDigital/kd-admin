{
	"ownerName": "KinsonDigital",
	"repoName": "kd-admin",
	"milestoneName": "${VERSION}",
	"githubTokenEnvVarName": "CICD_TOKEN",
	"environment": "Preview",
	"headerText": "${REPONAME} ${ENVIRONMENT} Release Notes - ${VERSION}",
	"extraInfo": {
		"title": "Quick Reminder",
		"text": "As with all software, there is always a chance for issues and bugs, especially for preview releases, so your input is greatly appreciated. 🙏🏼"
	},
	"emojisToRemoveFromTitle": ["🚧", "🐛", "✨", "🚀", "🧪", "🔬"],
	"issueCategoryLabelMappings": {
		"New Features ✨": "new-feature",
		"Enhancements ✨": "enhancement",
		"Bug Fixes 🐛": "bug",
		"Breaking Changes 🧨": "🧨breaking-changes",
		"Dependency Updates 📦": "📦dependency-update"
	},
	"prCategoryLabelMappings": {
		"Dependency Updates 📦": "📦dependency-update"
	},
	"ignoreLabels": ["🚀Preview Release", "🚀Production Release"],
	"wordReplacements": {
		"chore(deps): ": ""
	},
	"firstWordReplacements": {
		"Add": "Added",
		"Improve": "Improved",
		"Fix": "Fixed",
		"Update": "Updated",
		"Upgrade": "Upgraded",
		"Create": "Created",
		"Refactor": "Refactored",
		"Removed": "Removed",
		"Implement": "Implemented",
		"Move": "Moved",
		"Change": "Changed",
		"Increase": "Increased",
		"Prevent": "Prevented",
		"Introduce": "Introduced",
		"Adjust": "Adjusted",
		"Replace": "Replaced",
		"Deprecate": "Deprecated",
		"Integrate": "Integrated"
	},
	"styleWordsList": {
		"xunit-dotnet monorepo": "bold,italic",
		"sixlabors.imagesharp": "bold,italic",
		"CASL": "bold,italic",
		"freetypesharp": "bold,italic",
		"KinsonDigital.Carbonate": "bold,italic",
		"system.io.abstractions": "bold,italic",
		"coverlet.msbuild": "bold,italic",
		"coverlet.collector": "bold,italic"
	},
    "boldedVersions": true,
    "italicVersions": true,
    "otherCategoryName": "Other 🪧"
}
